<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="1000.0" spacing="20.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.SignUpController">
   <children>
      <Label layoutX="276.0" layoutY="90.0" text="Welcome To Hibernia-Sino Insurance" textAlignment="CENTER" textFill="#0066cc">
         <font>
            <Font name="Microsoft YaHei Bold" size="30.0" />
         </font>
         <VBox.margin>
            <Insets top="-50.0" />
         </VBox.margin>
      </Label>
      <TabPane id="tab_pane" layoutX="279.0" layoutY="158.0" maxHeight="300.0" maxWidth="500.0" prefHeight="300.0" prefWidth="500.0" stylesheets="@style.css" tabClosingPolicy="UNAVAILABLE" tabMaxHeight="20.0" tabMaxWidth="100.0">
         <tabs>
            <Tab fx:id="typename" text="client">
               <content>
                  <Pane id="pane_background" prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <Group layoutX="25.0" layoutY="-5.0">
                           <children>
                              <Label layoutX="25.0" layoutY="133.0" text="Confirm Password">
                                 <font>
                                    <Font name="Microsoft YaHei Bold" size="18.0" />
                                 </font>
                              </Label>
                              <Label layoutX="104.0" layoutY="93.0" text="Password">
                                 <font>
                                    <Font name="Microsoft YaHei Bold" size="18.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="username" layoutX="195.0" layoutY="50.0" prefWidth="230.0" />
                              <Label layoutX="99.0" layoutY="52.0" text="Username">
                                 <font>
                                    <Font name="Microsoft YaHei Bold" size="18.0" />
                                 </font>
                              </Label>
                              <PasswordField fx:id="password" layoutX="195.0" layoutY="90.0" prefWidth="230.0" />
                              <PasswordField fx:id="password2" layoutX="195.0" layoutY="128.0" prefWidth="230.0" />
                           </children>
                        </Group>
                        <Button fx:id="bt_signUp" layoutX="151.0" layoutY="176.0" mnemonicParsing="false" onAction="#signUp" text="Sign Up" textFill="WHITE">
                           <font>
                              <Font name="Microsoft YaHei Bold" size="18.0" />
                           </font>
                        </Button>
                        <Button fx:id="bt_back" layoutX="274.0" layoutY="176.0" mnemonicParsing="false" onAction="#toSignInView" text="Back" textFill="WHITE" />
                     </children>
                  </Pane>
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </children>
</VBox>
