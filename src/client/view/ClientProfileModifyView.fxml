<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<HBox alignment="TOP_CENTER" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.view.ClientProfileModifyController">
   <children>
      <ScrollPane minWidth="1000.0" stylesheets="@style.css">
         <content>
            <VBox id="stage" alignment="CENTER" minHeight="600.0" minWidth="1000.0" stylesheets="@style.css">
               <children>
                  <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
                     <children>
                        <VBox id="group" spacing="20.0" stylesheets="@style.css">
                           <children>
                              <Label text="Client Service" textFill="#0066cc">
                                 <font>
                                    <Font name="Microsoft YaHei Bold" size="20.0" />
                                 </font>
                              </Label>
                              <Line endX="205.0" startX="55.0" stroke="#0066cc" strokeMiterLimit="0.0" />
                              <Button fx:id="btn_home" onAction="#toHome" stylesheets="@style.css" text="Home" textFill="#0066cc">
                                 <font>
                                    <Font name="Microsoft YaHei" size="20.0" />
                                 </font>
                                 <padding>
                                    <Insets top="10.0" />
                                 </padding>
                              </Button>
                              <Line endX="205.0" startX="55.0" stroke="#0066cc" strokeMiterLimit="0.0" strokeWidth="2.0" />
                              <Button fx:id="btn_myInsurance" onAction="#backToInsurance" stylesheets="@style.css" text="My Insurances" textFill="#0066cc">
                                 <font>
                                    <Font name="Microsoft YaHei" size="20.0" />
                                 </font>
                                 <padding>
                                    <Insets top="10.0" />
                                 </padding>
                              </Button>
                              <Line endX="205.0" startX="55.0" stroke="#0066cc" strokeMiterLimit="0.0" />
                              <Button fx:id="btn_myProfile" onAction="#toClaimView" stylesheets="@style.css" text="My Profile" textFill="#0066cc">
                                 <font>
                                    <Font name="Microsoft YaHei" size="20.0" />
                                 </font>
                                 <padding>
                                    <Insets top="10.0" />
                                 </padding>
                              </Button>
                              <Line endX="205.0" startX="55.0" stroke="#0066cc" strokeMiterLimit="0.0" />
                              <Button fx:id="btn_myClaim" onAction="#toProfile" stylesheets="@style.css" text="Claim Settlement" textFill="#0066cc">
                                 <font>
                                    <Font name="Microsoft YaHei" size="20.0" />
                                 </font>
                                 <padding>
                                    <Insets top="10.0" />
                                 </padding>
                              </Button>
                              <Line endX="205.0" startX="55.0" stroke="#0066cc" strokeMiterLimit="0.0" />
                           </children>
                           <HBox.margin>
                              <Insets />
                           </HBox.margin></VBox>
                        <VBox prefHeight="600.0" spacing="35.0">
                           <children>
                              <Label text="Client Profile" textFill="#0066cc">
                                 <font>
                                    <Font name="Microsoft YaHei Bold" size="20.0" />
                                 </font>
                              </Label>
                              <HBox prefHeight="100.0" prefWidth="200.0" spacing="15.0">
                                 <children>
                                    <VBox spacing="25.0">
                                       <children>
                                          <Label text="First Name:">
                                             <font>
                                                <Font name="Microsoft YaHei Bold" size="16.0" />
                                             </font>
                                          </Label>
                                          <Label text="Gender:">
                                             <font>
                                                <Font name="Microsoft YaHei Bold" size="16.0" />
                                             </font>
                                          </Label>
                                          <Label text="ID:">
                                             <font>
                                                <Font name="Microsoft YaHei Bold" size="16.0" />
                                             </font>
                                          </Label>
                                          <Label text="Province:">
                                             <font>
                                                <Font name="Microsoft YaHei Bold" size="16.0" />
                                             </font>
                                          </Label>
                                          <Label text="Email:">
                                             <font>
                                                <Font name="Microsoft YaHei Bold" size="16.0" />
                                             </font>
                                          </Label>
                                       </children>
                                    </VBox>
                                    <VBox spacing="18.0">
                                       <children>
                                          <TextField fx:id="firstName" />
                                          <HBox spacing="25.0">
                                             <children>
                                                <RadioButton fx:id="btn_male" mnemonicParsing="false" text="Male" />
                                                <RadioButton fx:id="btn_female" mnemonicParsing="false" text="Female" />
                                             </children>
                                          </HBox>
                                          <TextField fx:id="socialId" />
                                          <TextField fx:id="province" />
                                          <TextField fx:id="email" />
                                       </children>
                                    </VBox>
                                    <VBox prefHeight="200.0" prefWidth="100.0" spacing="25.0">
                                       <children>
                                          <Label text="Last Name:">
                                             <font>
                                                <Font name="Microsoft YaHei Bold" size="16.0" />
                                             </font>
                                          </Label>
                                          <Label text="Age:">
                                             <font>
                                                <Font name="Microsoft YaHei Bold" size="16.0" />
                                             </font>
                                          </Label>
                                          <Label text="Country:">
                                             <font>
                                                <Font name="Microsoft YaHei Bold" size="16.0" />
                                             </font>
                                          </Label>
                                          <Label text="City:">
                                             <font>
                                                <Font name="Microsoft YaHei Bold" size="16.0" />
                                             </font>
                                          </Label>
                                          <Label text="Phone:">
                                             <font>
                                                <Font name="Microsoft YaHei Bold" size="16.0" />
                                             </font>
                                          </Label>
                                       </children>
                                    </VBox>
                                    <VBox spacing="15.0">
                                       <children>
                                          <TextField fx:id="lastName" />
                                          <TextField fx:id="age" />
                                          <TextField fx:id="country" />
                                          <TextField fx:id="city" />
                                          <TextField fx:id="phone" />
                                       </children>
                                    </VBox>
                                 </children>
                              </HBox>
                              <HBox id="pane_background" alignment="TOP_CENTER" prefWidth="700.0" stylesheets="@style.css">
                                 <children>
                                    <Button fx:id="btn_save" mnemonicParsing="false" onAction="#saveProfile" prefHeight="30.0" prefWidth="150.0" text="Save" textFill="WHITE" />
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </children>
</HBox>
