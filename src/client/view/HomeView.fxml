<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<HBox alignment="TOP_CENTER" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.view.HomeController">
   <children>
      <ScrollPane fx:id="pane" minWidth="1000.0">
         <content>
            <VBox id="stage" alignment="TOP_CENTER" minWidth="1000.0" stylesheets="@style.css">
               <children>
                  <ImageView fitHeight="501.0" fitWidth="900.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../resources/pic/company.png" />
                     </image>
                  </ImageView>
                  <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="25.0">
                     <children>
                        <VBox id="group" spacing="20.0" stylesheets="@style.css">
                           <children>
                              <Label text="Client Service" textFill="#0066cc">
                                 <font>
                                    <Font name="Microsoft YaHei Bold" size="20.0" />
                                 </font>
                              </Label>
                              <Line endX="205.0" startX="55.0" stroke="#0066cc" strokeMiterLimit="0.0" />
                              <Button fx:id="btn_home" onAction="#toHome" stylesheets="@style.css" text="Home" textFill="#0066cc">
                                 <font>
                                    <Font name="Microsoft YaHei" size="20.0" />
                                 </font>
                                 <padding>
                                    <Insets top="10.0" />
                                 </padding>
                              </Button>
                              <Line endX="205.0" startX="55.0" stroke="#0066cc" strokeMiterLimit="0.0" strokeWidth="2.0" />
                              <Button fx:id="btn_myInsurance" onAction="#backToInsurance" stylesheets="@style.css" text="My Insurances" textFill="#0066cc">
                                 <font>
                                    <Font name="Microsoft YaHei" size="20.0" />
                                 </font>
                                 <padding>
                                    <Insets top="10.0" />
                                 </padding>
                              </Button>
                              <Line endX="205.0" startX="55.0" stroke="#0066cc" strokeMiterLimit="0.0" />
                              <Button fx:id="btn_myProfile" onAction="#toProfile" stylesheets="@style.css" text="My Profile" textFill="#0066cc">
                                 <font>
                                    <Font name="Microsoft YaHei" size="20.0" />
                                 </font>
                                 <padding>
                                    <Insets top="10.0" />
                                 </padding>
                              </Button>
                              <Line endX="205.0" startX="55.0" stroke="#0066cc" strokeMiterLimit="0.0" />
                              <Button fx:id="btn_insuranceClaim" onAction="#toClaimView" stylesheets="@style.css" text="Claim Settlement" textFill="#0066cc">
                                 <font>
                                    <Font name="Microsoft YaHei" size="20.0" />
                                 </font>
                                 <padding>
                                    <Insets top="10.0" />
                                 </padding>
                              </Button>
                              <Line endX="205.0" startX="55.0" stroke="#0066cc" strokeMiterLimit="0.0" />
                           </children>
                           <HBox.margin>
                              <Insets />
                           </HBox.margin></VBox>
                        <VBox id="pane_background" alignment="TOP_CENTER" spacing="10.0" stylesheets="@style.css">
                           <children>
                              <Text fill="#666666" strokeType="OUTSIDE" strokeWidth="0.0" text="When you travel on three of the most common modes of transport, train, plane, and long-distance bus, do you worry about the loss of your belongings as a result of an accident? Our basic insurance will be a good escort for your luggage to help you stop damage in time." textAlignment="CENTER" wrappingWidth="211.60498046875">
                                 <font>
                                    <Font name="Microsoft YaHei Bold" size="18.0" />
                                 </font></Text>
                              <Button fx:id="bt_plan1" mnemonicParsing="false" onAction="#toPlan1" prefHeight="30.0" prefWidth="150.0" text="Plan I" textFill="WHITE">
                                 <VBox.margin>
                                    <Insets />
                                 </VBox.margin></Button>
                           </children>
                        </VBox>
                        <Line endY="360.0" stroke="#0066cc" strokeLineCap="ROUND" strokeLineJoin="ROUND" />
                        <VBox id="pane_background" alignment="TOP_CENTER" spacing="30.0" stylesheets="@style.css">
                           <children>
                              <Text fill="#666666" strokeType="OUTSIDE" strokeWidth="0.0" text="When you travel by train, plane, long-distance bus or taxi, how can you remedy the damage caused by the loss of your luggage in the event of natural and man-made disasters? This quality insurance can provide you with better protection." textAlignment="CENTER" wrappingWidth="199.60498046875">
                                 <font>
                                    <Font name="Microsoft YaHei Bold" size="18.0" />
                                 </font>
                              </Text>
                              <Button fx:id="bt_plan2" mnemonicParsing="false" onAction="#toPlan2" prefHeight="30.0" prefWidth="150.0" text="Plan II" textFill="WHITE">
                                 <VBox.margin>
                                    <Insets />
                                 </VBox.margin></Button>
                           </children>
                        </VBox>
                        <Line endY="360.0" stroke="#0066cc" strokeLineCap="ROUND" strokeLineJoin="ROUND" />
                        <VBox id="pane_background" alignment="TOP_CENTER" spacing="30.0" stylesheets="@style.css">
                           <children>
                              <Text fill="#666666" strokeType="OUTSIDE" strokeWidth="0.0" text="When you travel by train, plane, coach, taxi or steamship, if your luggage is lost due to fire, explosion, torrential rain and so on, how are you going to recover the loss? The supreme insurance products we offer must be your best choice." textAlignment="CENTER" wrappingWidth="201.60498046875">
                                 <font>
                                    <Font name="Microsoft YaHei Bold" size="18.0" />
                                 </font>
                              </Text>
                              <Button fx:id="bt_plan3" mnemonicParsing="false" onAction="#toPlan3" prefHeight="30.0" prefWidth="150.0" text="Plan III" textFill="WHITE" />
                           </children>
                        </VBox>
                     </children>
                     <VBox.margin>
                        <Insets top="50.0" />
                     </VBox.margin>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="80.0" top="50.0" />
               </padding>
            </VBox>
         </content>
      </ScrollPane>
   </children>
</HBox>
